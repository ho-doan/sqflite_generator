  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Widgetbook Generator",
      "cwd": "example",
      "request": "launch",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "type": "dart",
      "args": ["build", "--delete-conflicting-outputs"]
    },
    {
      "name": "sqflite_generator",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "example",
      "cwd": "example",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "example (profile mode)",
      "cwd": "example",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "example (release mode)",
      "cwd": "example",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "sqflite_annotation",
      "cwd": "packages/sqflite_annotation",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "sqflite_query",
      "cwd": "packages/sqflite_query",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "sqflite_query (profile mode)",
      "cwd": "packages/sqflite_query",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "sqflite_query (release mode)",
      "cwd": "packages/sqflite_query",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
